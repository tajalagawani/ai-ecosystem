{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["{{", "}}"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["{{", "}}"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["{{", "}}"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\[.*\\]\\s*$",
    "decreaseIndentPattern": "^\\s*$"
  },
  "wordPattern": "\\b[A-Za-z_][A-Za-z0-9_]*\\b"
}
